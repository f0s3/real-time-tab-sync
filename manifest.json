{
  "name": "Real-Time Tab Sync",
  "version": "1.3.5",
  "manifest_version": 2,
  "description": "Keeps the same tabs on all your machines. Tab changes are applied in real time on all of your devices.",
  "icons": {
  	"16": "img/icon16.png",
  	"48": "img/icon48.png",
  	"128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon19grey.png",
      "38": "img/icon38grey.png"
    },
    "default_title": "Default",
	  "default_popup": "popup.html"
  },
  "background": {
	"scripts": [
    "script/background.js",
    "script/common.js"
  ],
	"persistent": true
  },
  "permissions": [
  	"storage",
  	"tabs",
    "webRequest",
    "http://*/*",
    "https://*/*"
  ]
}
